	partition 'Top N Products (By Profit)' = calculated
		mode: import
		source = ```
				
				VAR chosen = SELECTEDVALUE('Top N'[Value])
				
				RETURN
				TOPN(
				    5,
				    SUMMARIZE(
				        sales_items,
				        sales_items[productId],
				        sales_items[productName],
				        "Profit", SUM(sales_items[margin])
				    ),
				    [Profit],
