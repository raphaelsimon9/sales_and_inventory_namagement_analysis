	measure 'Total Customers Till Date' =
			
			COALESCE (
			    CALCULATE (
			        DISTINCTCOUNT ( customers[customerId] ),
			        USERELATIONSHIP ( 'Calendar'[Date], customers[registrationDate] ),
			        FILTER ( ALL ( 'Calendar' ), 'Calendar'[Date] <= MAX ( 'Calendar'[Date] ) )
			    ),
			    0
			)
		formatString: #,0
		displayFolder: Customer Insights
		lineageTag: 74437511-2ac6-48d5-8aa3-7a3f113af833

	measure 'Total Revenue' =
			
			CALCULATE (
			    SUMX ( sales_items, sales_items[totalAmount] ),
			    USERELATIONSHIP ( 'Calendar'[Date], sales_items[orderDate] )
			)
		formatString: \$#,0.00;(\$#,0.00);\$#,0.00
